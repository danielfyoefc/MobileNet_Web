@{
    Layout = null;
}
    <h2>Company</h2>
        <div class="form-horizontal">
            <ul id="validationSummary"></ul>
            Are you sure that you want to delete {{Company}}?
            <div class="form-group">
                <label for="Name" class = "control-label col-md-2">
                Name
                </label>
                <div class="col-md-10">
                    {{data.Name}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="TerminalId" class = "control-label col-md-2">
                TerminalId
                </label>
                <div class="col-md-10">
                    {{data.TerminalId}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Address" class = "control-label col-md-2">
                Address
                </label>
                <div class="col-md-10">
                    {{data.Address}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="PhoneNumber" class = "control-label col-md-2">
                PhoneNumber
                </label>
                <div class="col-md-10">
                    {{data.PhoneNumber}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="MobileNumber" class = "control-label col-md-2">
                MobileNumber
                </label>
                <div class="col-md-10">
                    {{data.MobileNumber}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="CompanyType" class = "control-label col-md-2">
                CompanyType
                </label>
                <div class="col-md-10">
                    {{data.CompanyType}}
                </div>
            </div>
    
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Save" class="btn btn-default" ng-click="save()" />
                </div>
            </div>
        </div>
    
    <div>
        <a href="#/Company">Back to List</a>
    </div>
